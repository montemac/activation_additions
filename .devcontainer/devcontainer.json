{
  "name": "PyTorch Dev Container",
  "dockerFile": "Dockerfile",
  "context": "..",
  "workspaceFolder": "/home/vscode/activation_additions",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "extensions": [
    "ms-python.python",
    "ms-vscode.cpptools"
  ]
}
